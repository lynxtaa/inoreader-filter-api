{
  "name": "inoreader-filter-api",
  "version": "0.3.0",
  "description": "Filter RSS Articles",
  "private": true,
  "main": "index.js",
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "repository": "https://github.com/lynxtaa/inoreader-filter-api.git",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "nodemonConfig": {
    "ext": "ts,js",
    "exec": "ts-node --files=false src/index.ts"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.2",
    "fastify": "^2.14.1",
    "fastify-gql": "^3.8.0",
    "fastify-plugin": "^1.6.1",
    "fastify-secure-session": "^1.2.0",
    "fastify-static": "^2.7.0",
    "glob": "^7.1.6",
    "graphql-middleware": "^4.0.2",
    "graphql-scalars": "^1.1.2",
    "graphql-shield": "^7.3.0",
    "graphql-tools": "^5.0.0",
    "http-errors": "^1.7.3",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.0",
    "objection": "^2.1.3",
    "pg": "^8.2.0",
    "point-of-view": "^3.8.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.151",
    "@types/ms": "^0.7.31",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "migrate:make": "knex migrate:make --knexfile ./src/knexfile.js",
    "debug": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ext .ts src",
    "test": "node jest.js"
  }
}
