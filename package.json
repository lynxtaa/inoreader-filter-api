{
  "name": "inoreader-filter",
  "version": "0.6.0",
  "private": true,
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "description": "Filter RSS Articles",
  "repository": "https://github.com/lynxtaa/inoreader-filter.git",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "clearMocks": true
  },
  "prettier": {
    "printWidth": 90,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@chakra-ui/core": "0.8.0",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "cross-env": "7.0.2",
    "date-fns": "2.16.1",
    "dotenv-safe": "8.2.0",
    "emotion-theming": "10.0.27",
    "fastify": "3.5.0",
    "fastify-nextjs": "5.0.4",
    "fastify-static": "3.2.0",
    "http-errors": "1.8.0",
    "mongoose": "5.10.7",
    "ms": "2.1.2",
    "next": "9.5.3",
    "node-fetch": "2.6.1",
    "pino": "6.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "6.8.6",
    "swr": "0.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.6",
    "@types/http-errors": "1.8.0",
    "@types/jest": "26.0.14",
    "@types/mongoose": "5.7.36",
    "@types/ms": "0.7.31",
    "@types/node": "12.12.62",
    "@types/node-fetch": "2.5.7",
    "@types/pino": "6.3.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-eslint": "10.1.0",
    "concurrently": "5.3.0",
    "eslint": "7.10.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.4.2",
    "msw": "0.21.2",
    "pino-pretty": "4.2.1",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "tsc-watch": "4.2.9",
    "typescript": "4.0.3",
    "whatwg-fetch": "3.4.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development tsc-watch --project api/tsconfig.json --onSuccess \"node dist/index.js\"",
    "lint": "eslint --ext .ts,.js api src",
    "validate": "concurrently \"npm:lint\" \"npm:check-format\"",
    "prettier": "prettier src api *.{md,js}",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "build": "concurrently \"npm:build:*\"",
    "build:server": "tsc --project api/tsconfig.json",
    "build:web": "next build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  }
}
