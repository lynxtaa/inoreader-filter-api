{
  "name": "inoreader-filter-api",
  "version": "0.3.0",
  "description": "Filter RSS Articles",
  "private": true,
  "main": "index.js",
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "repository": "https://github.com/lynxtaa/inoreader-filter-api.git",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.2",
    "fastify": "^2.14.1",
    "fastify-gql": "^3.8.0",
    "fastify-plugin": "^1.6.1",
    "fastify-secure-session": "^1.2.0",
    "fastify-static": "^2.7.0",
    "glob": "^7.1.6",
    "graphql-middleware": "^4.0.2",
    "graphql-scalars": "^1.1.2",
    "graphql-shield": "^7.3.0",
    "graphql-tools": "^5.0.0",
    "http-errors": "^1.7.3",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.14",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.0",
    "point-of-view": "^3.8.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.151",
    "@types/mongoose": "^5.7.20",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.39",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "@types/ms": "^0.7.31",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "build": "tsc --project tsconfig.server.json",
    "watch:ts": "tsc --project tsconfig.server.json --watch",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon dist/index.js",
    "debug": "run-p start:dev watch:ts",
    "lint": "eslint --ext .ts src",
    "web:start": "react-scripts start",
    "web:build": "react-scripts build",
    "web:test": "react-scripts test",
    "web:eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
