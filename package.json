{
  "name": "inoreader-filter",
  "version": "0.8.1",
  "private": true,
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "description": "Filter RSS Articles",
  "repository": "https://github.com/lynxtaa/inoreader-filter.git",
  "license": "MIT",
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "prettier": "@lynxtaa/prettier-config",
  "dependencies": {
    "@chakra-ui/icons": "1.1.5",
    "@chakra-ui/react": "1.8.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@typegoose/typegoose": "9.6.2",
    "axios": "^0.25.0",
    "cross-env": "7.0.3",
    "date-fns": "2.28.0",
    "dotenv-safe": "8.2.0",
    "fastify": "3.27.1",
    "fastify-nextjs": "7.2.0",
    "fastify-static": "4.5.0",
    "framer-motion": "6.2.4",
    "http-errors": "2.0.0",
    "mongoose": "6.2.1",
    "ms": "2.1.3",
    "next": "12.0.10",
    "node-fetch": "2.6.7",
    "pino": "7.6.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.26.1",
    "react-query": "^3.34.14"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.2",
    "@lynxtaa/prettier-config": "^0.1.1",
    "@next/bundle-analyzer": "^12.0.10",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/http-errors": "1.8.2",
    "@types/jest": "27.4.0",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.24",
    "@types/node-fetch": "2.5.12",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "concurrently": "7.0.0",
    "eslint": "8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-plugin-import": "2.25.4",
    "is-ci-cli": "2.2.0",
    "jest": "27.5.1",
    "msw": "0.36.8",
    "pino-pretty": "7.5.1",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "simple-git-hooks": "^2.7.0",
    "tsc-watch": "4.6.0",
    "typescript": "4.5.5",
    "whatwg-fetch": "3.6.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development tsc-watch --project api/tsconfig.json --onSuccess \"node dist/index.js\"",
    "lint": "eslint --ext .ts,.js,.tsx api src pages",
    "validate": "concurrently \"npm:lint\" \"npm:check-format\"",
    "prettier": "prettier src api pages *.{md,js}",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "build": "concurrently \"npm:build:*\"",
    "build:server": "tsc --project api/tsconfig.json",
    "build:web": "next build",
    "analyze:web": "cross-env ANALYZE=true npm run build:web",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  }
}
