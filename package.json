{
  "name": "inoreader-filter",
  "version": "0.7.0",
  "private": true,
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "description": "Filter RSS Articles",
  "repository": "https://github.com/lynxtaa/inoreader-filter.git",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=12.x"
  },
  "jest": {
    "clearMocks": true
  },
  "prettier": {
    "printWidth": 90,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.3",
    "@chakra-ui/react": "1.1.5",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@typegoose/typegoose": "7.4.8",
    "axios": "^0.21.1",
    "cross-env": "7.0.3",
    "date-fns": "2.16.1",
    "dotenv-safe": "8.2.0",
    "fastify": "3.10.1",
    "fastify-nextjs": "5.1.0",
    "fastify-static": "3.4.0",
    "framer-motion": "3.2.1",
    "http-errors": "1.8.0",
    "mongoose": "5.10.18",
    "ms": "2.1.3",
    "next": "10.0.5",
    "node-fetch": "2.6.1",
    "pino": "6.11.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.14.2",
    "react-query": "^3.5.16"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.12",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/http-errors": "1.8.0",
    "@types/jest": "26.0.20",
    "@types/mongoose": "5.10.3",
    "@types/ms": "0.7.31",
    "@types/node": "14.14.21",
    "@types/node-fetch": "2.5.8",
    "@types/pino": "6.3.5",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-typescript-metadata": "0.3.1",
    "concurrently": "5.3.0",
    "eslint": "7.18.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.2.0",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "msw": "0.25.0",
    "pino-pretty": "4.3.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "tsc-watch": "4.2.9",
    "typescript": "4.1.3",
    "whatwg-fetch": "3.5.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development tsc-watch --project api/tsconfig.json --onSuccess \"node dist/index.js\"",
    "lint": "eslint --ext .ts,.js,.tsx api src pages",
    "validate": "concurrently \"npm:lint\" \"npm:check-format\"",
    "prettier": "prettier src api *.{md,js}",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "build": "concurrently \"npm:build:*\"",
    "build:server": "tsc --project api/tsconfig.json",
    "build:web": "next build",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  }
}
